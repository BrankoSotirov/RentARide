@model IEnumerable<VehicleServiceModel>
@{
	ViewBag.Title = "My Vehicles";
}
<h2 class="text-center">>@ViewBag.Title</h2>
<hr />

@if(Model.Any() == false)
{
	
	<h3 class="text-center"> You do not have any vehicles at the moment!</h3>
}

<div class=""row">

	@foreach(var item in Model)
	{
		<partial name="_VehiclePartial" model = "@item" />
	}

</div>